<%= form_tag log_in_path, :id => "log_in" do %> 
  <h1>OhWiki</h1>
    <%= label_tag :email %>
    <%= text_field_tag :email, "", :placeholder => "Email address" %>
    <%= label_tag :password %>
    <%= text_field_tag :password,"", :placeholder => "Password", :type => "password" %>
    <%= submit_tag 'Log in', :id => 'submit' %>
<% end %>
